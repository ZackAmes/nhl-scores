{
  "name": "fresh",
  "module": "index.ts",
  "type": "module",
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.1",
    "bun-types": "latest",
    "vite": "^5.0.12"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@types/express": "^4.17.21",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^6.1.8",
    "vite-plugin-top-level-await": "^1.3.1",
